        <rdfs:subClassOf rdf:resource="http://www.plminterop.org/archimate/Realization_Type1"/>
    </owl:Class>
    


    <!-- http://www.plminterop.org/archimate/StructuralRelationship -->

    <owl:Class rdf:about="http://www.plminterop.org/archimate/StructuralRelationship">
        <rdfs:subClassOf rdf:resource="http://www.plminterop.org/archimate/ArchiMateModelingRelation"/>
    </owl:Class>
    


    <!-- http://www.plminterop.org/archimate/StructureElement -->

    <owl:Class rdf:about="http://www.plminterop.org/archimate/StructureElement">
        <rdfs:subClassOf rdf:resource="http://www.plminterop.org/archimate/ArchiMateModelingEntity"/>
        <rdfs:comment xml:lang="en">These three aspects – active structure, behavior, and passive structure – have been inspired by natural language, where a sentence has a subject (active structure), a verb (behavior), and an object (passive structure).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.plminterop.org/archimate/SystemOfInterest -->

    <owl:Class rdf:about="http://www.plminterop.org/archimate/SystemOfInterest">
